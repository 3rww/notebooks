## SETUP

# create the conda environment
echo y | conda create --name rainways-nbs
# activate the environment
activate rainways-nbs
# install packages
echo y | conda install --name rainways-nbs -c esri arcgis
echo y | conda install --name rainways-nbs -c conda-forge geopandas 
echo y | conda install --name rainways-nbs -c conda-forge requests